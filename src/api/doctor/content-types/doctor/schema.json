{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "doctor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "specialty": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "credentials": {
      "type": "json"
    },
    "experience": {
      "type": "string"
    },
    "languages": {
      "type": "json"
    },
    "email": {
      "type": "email",
      "required": false,
      "unique": true
    },
    "phone": {
      "type": "string"
    },
    "blog_posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "author"
    },
    "appointments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor"
    },
    "schedule": {
      "type": "json"
    }
  }
}
