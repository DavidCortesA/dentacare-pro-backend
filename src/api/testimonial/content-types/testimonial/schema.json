{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "testimonial"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 5
    },
    "comment": {
      "type": "text",
      "required": true
    },
    "treatment": {
      "type": "string"
    },
    "verified": {
      "type": "boolean",
      "default": true
    }
  }
}
